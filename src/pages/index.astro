---

import AllStars from '@components/themes/AllStars.astro';
import LinkTreeLayout from '../layouts/LinkTreeLayout.astro';

import { getConfig } from '../libs/config';
import BusinessCard from '@components/themes/BusinessCard.astro';
import Disco from '@components/themes/Disco.astro';

const config = getConfig();

---

<LinkTreeLayout page={config.page}>
	{config.theme === 'all-stars' && <AllStars config={config} />}
	{config.theme === 'business-card' && <BusinessCard config={config} />}
	{config.theme === 'disco' && <Disco config={config} />}
</LinkTreeLayout>